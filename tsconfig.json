{
    "compilerOptions": {
      "jsx": "react-jsx",
      "module": "es6",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "noImplicitThis": true,
      "outDir": "dist/js",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "target": "es5",
      "esModuleInterop": true,
      "rootDir": "./App",
    },
    "include": [
      "App"
    ]
  }